__pycache__/
*.py[cod]
*$py.class
*.so

# Django stuff
*.log
local_settings.py
db.sqlite3
db.sqlite3-journal

# Media files
media/
staticfiles/
static/

# Virtual environments
venv/
env/
ENV/
.venv/
.env
.env.local

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Python distribution
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST

# Archivos pesados AI/ML
*.pyd
*.dat
*.model
*.pkl
*.h5
*.hdf5

# Archivos de instalacion
*.msi
*.exe
*.dll
*.zip
*.tar.gz

# Node.js
node_modules/
package-lock.json

